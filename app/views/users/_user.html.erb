<tr>
  <td>
    <%= user.user_name %>
  </td>
  <td>
    <%= user.name %>
  </td>
  <td>
    <%= user.email %>
  </td>
  <td>
    <%= link_to 'Profile', user_path(user), class: 'btn btn-primary btn-xs' %>
  </td>
  <td>
    <% if (current_user.admin? && (!user.admin?)) then %>
    <%= link_to 'Delete', user, method: :delete, data: { confirm: 'Are you sure?' }, class: 'btn btn-danger btn-xs' %>
    <% end %>
  </td>          
</tr>
